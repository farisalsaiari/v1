{
    "compilerOptions": {
        "composite": true,
        "declaration": true,
        "declarationMap": true,
        "emitDeclarationOnly": true,
        "outDir": "dist",
        "rootDir": "src",
        "module": "ESNext",
        "target": "ES2020",
        "moduleResolution": "bundler",
        "jsx": "react",
        "jsxFactory": "h",
        "jsxFragmentFactory": "Fragment",
        "skipLibCheck": true,
        "strict": true
    },
    "include": [
        "src"
    ],
    "exclude": [
        "dist"
    ],
    "references": []
}